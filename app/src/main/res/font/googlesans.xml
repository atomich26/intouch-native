<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- REGULAR -->
    <font
        android:fontStyle="normal"
        android:fontWeight="400"
        android:font="@font/googlesans_regular" />
    <font
        android:fontStyle="italic"
        android:fontWeight="400"
        android:font="@font/googlesans_regular_italic" />

    <!-- MEDIUM -->
    <font
        android:fontStyle="normal"
        android:fontWeight="600"
        android:font="@font/googlesans_medium" />

    <font
        android:fontStyle="italic"
        android:fontWeight="600"
        android:font="@font/googlesans_medium_italic" />

    <!-- BOLD -->
    <font
        android:fontStyle="normal"
        android:fontWeight="800"
        android:font="@font/googlesans_bold" />
    <font
        android:fontStyle="italic"
        android:fontWeight="800"
        android:font="@font/googlesans_bold_italic" />

</font-family>